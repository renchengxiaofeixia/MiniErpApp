<view><view class="headline option-gooods"><text>选择物品</text><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="icon bg-green" bindtap="__e"><text class="icon-tianjia iconfont"></text><text>添加</text></view></view><block wx:for="{{productList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="product"><view class="product-show"><view class="goods-flex"><view class="product-btr"><text data-event-opts="{{[['tap',[['disposeOf',[index]]]]]}}" class="iconfont icon-guanbi1 red" bindtap="__e"></text><text class="name">{{item.prodName}}</text><text class="gray">{{"("+item.prodModel+")"}}</text></view><block wx:if="{{item.totalPrice}}"><view class="product-price green">{{'￥'+item.totalPrice+''}}</view></block></view><view class="serial gray">{{''+item.prodNo+''}}</view></view><block wx:if="{{ids!=4}}"><view class="goods-flex product-back product-show"><view class="product-title">单价</view><view class="product-sum"><input type="digit" placeholder="填写单价" data-event-opts="{{[['input',[['__set_model',['$0','purchasePrice','$event',[]],[[['productList','',index]]]],['numberFixedDigit',['$0'],[[['productList','',index]]]]]]]}}" value="{{item.purchasePrice}}" bindinput="__e"/></view></view></block><view class="goods-flex product-back product-show"><view class="product-title">数量</view><view class="product-sum"><view hidden="{{!(hide)}}"><uni-number-box vue-id="{{'7e5a6e17-1-'+index}}" min="{{1}}" max="{{1000}}" value="{{item.quantity}}" data-event-opts="{{[['^change',[['changeValue',['$0'],[[['productList','',index]]]]]],['^input',[['__set_model',['$0','quantity','$event',[]],[[['productList','',index]]]]]]]}}" bind:change="__e" bind:input="__e" bind:__l="__l"></uni-number-box></view><view hidden="{{!(!hide)}}"><uni-number-box vue-id="{{'7e5a6e17-2-'+index}}" min="{{1}}" max="{{item.notReceiverQuantity}}" value="{{item.quantity}}" data-event-opts="{{[['^change',[['changeValue',['$0'],[[['productList','',index]]]]]],['^input',[['__set_model',['$0','quantity','$event',[]],[[['productList','',index]]]]]]]}}" bind:change="__e" bind:input="__e" bind:__l="__l"></uni-number-box><text class="red" style="margin-left:10rpx;font-size:24rpx;"><block wx:if="{{item.notReceiverQuantity==item.quantity}}"><block>{{'还剩'+item.notReceiverQuantity+'可入库'}}</block></block><block wx:if="{{item.nowQuantity==item.quantity}}"><block>{{'库存：'+item.nowQuantity+''}}</block></block></text></view></view></view><view class="goods-flex product-back product-show"><view class="product-title">备注</view><view class="product-sum"><input type="text" placeholder="填写备注" data-event-opts="{{[['input',[['__set_model',['$0','newRemarks','$event',[]],[[['productList','',index]]]]]]]}}" value="{{item.newRemarks}}" bindinput="__e"/></view></view></view></block></view>