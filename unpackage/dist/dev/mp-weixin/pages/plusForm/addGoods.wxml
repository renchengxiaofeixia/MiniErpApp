<view><header-tab vue-id="adc5f3ae-1" title="新建物品" bind:__l="__l"></header-tab><view class="table"><view class="from from-new"><text class="title">物品编号</text><input class="fill" type="text" placeholder="填写编号,保存后不能修改 (必填)" data-event-opts="{{[['input',[['__set_model',['','productSeries','$event',[]]]]]]}}" value="{{productSeries}}" bindinput="__e"/></view><view class="from from-new"><text class="title">物品名称</text><input class="fill" type="text" placeholder="填写名称 (必填)" data-event-opts="{{[['input',[['__set_model',['','productName','$event',[]]]]]]}}" value="{{productName}}" bindinput="__e"/></view></view><view class="table"><view class="from from-new"><text class="title">规格型号</text><input class="fill" type="text" placeholder="填写品牌" data-event-opts="{{[['input',[['__set_model',['','productType','$event',[]]]]]]}}" value="{{productType}}" bindinput="__e"/></view><view class="from from-new"><text class="title">品牌</text><input class="fill" type="text" placeholder="填写品牌" data-event-opts="{{[['input',[['__set_model',['','productBrand','$event',[]]]]]]}}" value="{{productBrand}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="from from-new" bindtap="__e"><text class="title">单位</text><text class="fill">{{unit}}</text><text class="iconfont icon-right-1-copy"></text></view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="from from-new" bindtap="__e"><text class="title">类目</text><text class="fill">{{catCode}}</text><text class="iconfont icon-right-1-copy"></text></view></view><view class="headline">物品图片</view><view class="table uploading"><block wx:for="{{prodImage}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><image src="{{item}}" mode="aspectFill"></image></block></block><view data-event-opts="{{[['tap',[['addImg',['$event']]]]]}}" class="uploading-img" bindtap="__e">上传图片</view></view><view class="headline">价格信息</view><view class="table"><view class="from from-new"><text class="title">采购价</text><input class="fill" type="number" placeholder="填写采购价" data-event-opts="{{[['input',[['__set_model',['','purchasePrice','$event',[]]]]]]}}" value="{{purchasePrice}}" bindinput="__e"/></view><view class="from from-new"><text class="title">销售价</text><input class="fill" type="number" placeholder="填写销售价" data-event-opts="{{[['input',[['__set_model',['','salePrice','$event',[]]]]]]}}" value="{{salePrice}}" bindinput="__e"/></view></view><view class="headline">库存信息</view><view class="table"><view class="from from-new"><text class="title">库存下限</text><input class="fill" type="number" placeholder="填写库存下限" data-event-opts="{{[['input',[['__set_model',['','inventoryFloor','$event',[]]]]]]}}" value="{{inventoryFloor}}" bindinput="__e"/></view><view class="from from-new"><text class="title">库存上限</text><input class="fill" type="number" placeholder="填写库存上限" data-event-opts="{{[['input',[['__set_model',['','inventoryUpper','$event',[]]]]]]}}" value="{{inventoryUpper}}" bindinput="__e"/></view></view><view class="table"><view class="from from-new"><text class="title">备注</text><input class="fill" type="text" placeholder="填写备注" data-event-opts="{{[['input',[['__set_model',['','remarks','$event',[]]]]]]}}" value="{{remarks}}" bindinput="__e"/></view></view><view data-event-opts="{{[['tap',[['addProducts']]]]}}" class="newBtn" bindtap="__e"><button>确定</button></view></view>