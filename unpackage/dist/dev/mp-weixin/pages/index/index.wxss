.header {
  width: 100%;
  background-color: #2ea469;
  position: -webkit-sticky;
  position: sticky;
  top: 0;
}
.header .header-flex {
  display: flex;
}
.header .header-flex .iconfont {
  margin-left: 40rpx;
  color: #fff;
  font-size: 48rpx;
  font-weight: 600;
}
.header .header-flex .header-text {
  flex: 1;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
}
.blackboard {
  background: url(data:image/png;base64,) no-repeat 0 -240rpx;
  background-size: 100%;
  padding-top: 40rpx;
}
.blackboard .transaction {
  width: 650rpx;
  height: 320rpx;
  background-color: #fff;
  margin: auto;
  border-radius: 20rpx;
  box-shadow: -1px -3px 20px 1px #65616169;
  display: flex;
  align-items: center;
  padding: 30rpx 30rpx 34rpx;
}
.blackboard .transaction .left,
.blackboard .transaction .right {
  flex: 1;
  font-size: 28rpx;
  color: #848484;
}
.blackboard .transaction .right {
  text-align: center;
}
.blackboard .transaction .title {
  display: flex;
  align-items: center;
}
.blackboard .transaction .title .iconfont {
  margin-left: 10rpx;
}
.blackboard .transaction .money {
  font-weight: 700;
  margin: 40rpx 0;
  font-size: 54rpx;
  color: #000;
}
.blackboard .transaction .sum {
  font-size: 32rpx;
  color: #000;
  margin: 0 10rpx;
}
.blackboard .transaction .time {
  text-align-last: right;
}
.blackboard .transaction .bargain {
  margin: 50rpx 0;
}
.blackboard .baffle {
  height: 30rpx;
  width: 100%;
  background-color: #f2f2f2;
  box-shadow: 0px 6px 20px 1px #8a888830;
}
.shortcut {
  background-color: #fff;
}
.shortcut .headline {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  height: 80rpx;
  border-bottom: 1rpx solid #ccc;
  font-size: 28rpx;
}
.shortcut .headline .iconfont {
  font-size: 32rpx;
}
.shortcut .sudoku {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  padding-bottom: 40rpx;
}
.shortcut .sudoku .grid {
  font-size: 28rpx;
  color: #6b6b6b;
  text-align: center;
  width: 33%;
  margin-top: 30rpx;
}
.shortcut .sudoku .grid image {
  width: 60rpx;
  height: 60rpx;
}
.shortcut .sudoku .grid .title {
  margin-top: 12rpx;
}

