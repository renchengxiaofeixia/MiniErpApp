<view class="table"><view class="headline">{{''+title+''}}</view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view hover-class="checkActive"><view class="goods-flex back"><view><text>{{item.prodName}}</text><text class="gray">{{"("+item.prodModel+")"}}</text></view><block wx:if="{{hide}}"><view>{{'￥'+item.money+''}}</view></block><block wx:else><view class="green">{{'库存: '+item.actualNum+''}}</view></block></view><view class="come gray back">{{''+item.prodNo+''}}</view><block wx:if="{{hide}}"><view class="goods-flex" style="padding-bottom:14rpx;border-bottom:1rpx solid #ccc;"><view class="gray">{{'￥'+item.unitPrice+" x "+item.quantity+'件'}}</view><block wx:if="{{item.receiverStatus}}"><view class="gray">{{''+item.receiverStatus+''}}</view></block></view></block><block wx:else><view class="goods-flex">{{'计件：'+item.systemNum+''}}</view></block></view></block></block><block wx:if="{{hide}}"><view class="money"><text class="total">合计：</text><text class="green">{{"￥"+totalPrice}}</text></view></block></view>