<view class="paddingBottom"><header-tab vue-id="3c22e23f-1" title="采购订单详情" record="{{true}}" bind:__l="__l"></header-tab><view class="goods-list gray"><view class="goods-flex"><view><view class="track"><text>采购单号：</text><text class="black">{{purchase.purchaseNo}}</text></view><view class="track"><text>采购日期：</text><text class="black">{{purchase.purchaseOrderDate}}</text></view><view class="track"><text>交货日期：</text><text class="black">{{purchase.deliveryDeadlineDate}}</text></view></view><view class="storage">{{''+purchase.purchaseStatus+''}}</view></view></view><linkman vue-id="3c22e23f-2" list="{{supplierContact}}" bind:__l="__l"></linkman><product-message vue-id="3c22e23f-3" title="采购物品" list="{{goodsList}}" bind:__l="__l"></product-message><view class="table"><view class="goods-flex shove figure"><view>付款</view><view class="green">{{'￥'+paymentTotalPrice+''}}</view></view><block wx:for="{{paymentList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="goods-flex payment" hover-class="checkActive" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" data-event-params="{{({item})}}" bindtap="__e"><view class="gray"><view>{{''+item.paymentDate+''}}</view><view>{{''+item.remarks+''}}</view></view><view><view>{{'￥'+item.amount+''}}</view></view></view></block></block></view><view class="table"><uni-collapse vue-id="3c22e23f-4" bind:__l="__l" vue-slots="{{['default']}}"><uni-collapse-item vue-id="{{('3c22e23f-5')+','+('3c22e23f-4')}}" title="相关约定" bind:__l="__l" vue-slots="{{['default']}}"><view class="goods-flex shove"><view><view class="track"><text class="gray">结算方式：</text><text class="black">{{purchase.settlementMode}}</text></view><view class="track"><text class="gray">备注：</text><text class="black">{{purchase.remarks}}</text></view></view></view></uni-collapse-item></uni-collapse></view><operator vue-id="3c22e23f-6" list="{{operation}}" bind:__l="__l"></operator><copyreader vue-id="3c22e23f-7" show="{{compileShow}}" data-event-opts="{{[['^close',[['handleClose']]]]}}" bind:close="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="operation" hover-class="checkActive" data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" bindtap="__e">打印</view><block wx:if="{{!purchase.isAudit}}"><view class="operation" hover-class="checkActive" data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" bindtap="__e">修改</view></block><view class="operation red" hover-class="checkActive" data-event-opts="{{[['tap',[['purchaseDel',['$event']]]]]}}" bindtap="__e">删除</view><view class="operation" hover-class="checkActive" data-event-opts="{{[['tap',[['navStorage']]]]}}" bindtap="__e">入库</view><view class="operation" hover-class="checkActive" data-event-opts="{{[['tap',[['e3',['$event']]]]]}}" bindtap="__e">付款</view><block wx:if="{{!purchase.isAudit}}"><view class="operation" hover-class="checkActive" data-event-opts="{{[['tap',[['checkPurchase',['$event']]]]]}}" bindtap="__e">审核</view></block><block wx:else><view class="operation" hover-class="checkActive" data-event-opts="{{[['tap',[['anewPurchase',['$event']]]]]}}" bindtap="__e">反审核</view></block></copyreader><add-order vue-id="3c22e23f-8" type="2" data-event-opts="{{[['^click',[['handleClose']]]]}}" bind:click="__e" bind:__l="__l"></add-order></view>